<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.societyapp">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.CALL_PHONE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:networkSecurityConfig="@xml/network_management"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ui.share.TestActivity">

        </activity>
        <activity
            android:name=".ActivityNotification"
            android:parentActivityName=".Society_Dashboard_activity" />
        <activity
            android:name=".PostComDiscussActivty"
            android:parentActivityName=".DiscussionFormActivity" />
        <activity
            android:name=".DiscussionFormActivity"
            android:parentActivityName=".Society_Dashboard_activity" />
        <activity
            android:name=".Notice"
            android:parentActivityName=".Society_Dashboard_activity" />
        <activity
            android:name=".Staff_members_Activity"
            android:parentActivityName=".Society_Dashboard_activity" />
        <activity
            android:name=".Event"
            android:parentActivityName=".Society_Dashboard_activity" />
        <activity
            android:name=".Rules"
            android:parentActivityName=".Society_Dashboard_activity" />
        <activity
            android:name=".Help"
            android:parentActivityName=".Society_Dashboard_activity" />
        <activity
            android:name=".Maintenance"
            android:parentActivityName=".Society_Dashboard_activity" />
        <activity
            android:name=".Society_Dashboard_activity"
            android:label="@string/title_activity_society__dashboard_activity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".Forgetpassword" />
        <activity
            android:name=".SocietyServiceActivity"
            android:parentActivityName=".Society_Dashboard_activity" />
        <activity android:name=".Society_Registration" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>